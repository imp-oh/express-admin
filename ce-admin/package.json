{
  "name": "CodeEngineApi",
  "version": "1.5.0",
  "main": "index.js",
  "author": "ghost",
  "license": "MIT",
  "scripts": {
    "i": "yarn || npm i",
    "start": "node ./app.js",
    "prod": "cross-env NODE_ENV=production node-dev app.js",
    "dev": "node-dev app.js",
    "apidoc": "apidoc -i routes/ -o public/apidoc/",
    "build": "pkg -t win package.json"
  },
  "apidoc": {
    "title": "接口文档",
    "version": "1.0.0",
    "url": "http://localhost:3000"
  },
  "bin": "app.js",
  "pkg": {
    "scripts": [
      "api/**/*",
      "config/**/*",
      "routes/**/*",
      "utils/**/*",
      "app.js"
    ],
    "assets": []
  },
  "dependencies": {
    "bcrypt": "^5.0.1",
    "body-parser": "^1.19.1",
    "captchapng": "^0.0.1",
    "connect-multiparty": "^2.2.0",
    "express": "^4.17.2",
    "express-async-errors": "^3.1.1",
    "express-session": "^1.17.2",
    "formidable": "^2.0.1",
    "jsonwebtoken": "^8.5.1",
    "mysql": "^2.18.1",
    "node-rsa": "^1.1.1",
    "nodemailer": "^6.7.2",
    "redis": "3.0.0",
    "request-ip": "^2.1.3",
    "ua-parser-js": "^1.0.2",
    "uuid": "^8.3.2"
  }
}
